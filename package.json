  "scripts": {
    "build:lib": "babel src --out-dir lib",
    "build:dist": "webpack --config webpack.js",
    "build:dist:min": "NODE_ENV=production webpack --config webpack.js",
    "build": "npm run build:lib && npm run build:dist && npm run build:dist:min",
